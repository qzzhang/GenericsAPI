{
  "ver" : "",
  "authors" : ["tgu2"],
  "contact" : "http://kbase.us/contact-us/",
  "visible" : true,
  "categories" : ["active", "util"],
  "widgets": {
    "input": "kbaseNarrativeMethodInput",
    "output" : "no-display"
  },
  "parameters" : [
    {
      "id" : "matrix_obj_ref",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "text",
      "text_options" : {
        "valid_ws_types" : ["KBaseMatrices"]
      }
    },
    {
      "id" : "dimension",
      "optional" : false,
      "advanced" : true,
      "allow_multiple" : false,
      "default_values" : ["row"],
      "field_type" : "dropdown",
      "dropdown_options":
      {
        "options":
        [
          {
              "display": "Row",
              "value": "row"
          },
          {
              "display": "Column",
              "value": "col"
          }
        ]
      }
    },
    {
      "id" : "remove_ids",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "textarea",
      "text_options" : {
         "n_rows" : 2
      }
    },
    {
      "id" : "filtered_matrix_name",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "text",
      "text_options" : {
          "valid_ws_types" : ["KBaseMatrices"],
          "is_output_name" : true
        }
    }
  ],
  "behavior" : {
    "service-mapping" : {
      "url" : "",
      "name" : "GenericsAPI",
      "method" : "filter_matrix",
      "input_mapping" : [
        {
          "narrative_system_variable": "workspace",
          "target_property": "workspace_name"
        },
        {
          "input_parameter" : "matrix_obj_ref",
          "target_property" : "matrix_obj_ref",
          "target_type_transform": "resolved-ref"
        },
        {
          "input_parameter" : "remove_ids",
          "target_property" : "remove_ids"
        },
        {
          "input_parameter" : "dimension",
          "target_property" : "dimension"
        },
        {
          "input_parameter" : "filtered_matrix_name",
          "target_property" : "filtered_matrix_name"
        }
      ],
      "output_mapping" : [
        {
          "narrative_system_variable": "workspace",
          "target_property": "wsName"
        },
        {
            "service_method_output_path": [0, "matrix_obj_refs"],
            "target_property": "matrix_obj_refs"
        },
        {
            "service_method_output_path": [0, "report_name"],
            "target_property": "report_name"
        },
        {
            "service_method_output_path": [0, "report_ref"],
            "target_property": "report_ref"
        },
        {
            "constant_value": "16",
            "target_property": "report_window_line_height"
        }
      ]
    }
  },
  "job_id_output_field": "docker"
}
