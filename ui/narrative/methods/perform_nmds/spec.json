{
  "ver" : "",
  "authors" : ["qzhang"],
  "contact" : "http://kbase.us/contact-us/",
  "visible" : true,
  "categories" : ["active","util"],
  "widgets": {
    "input": "kbaseNarrativeMethodInput",
    "output" : "no-display"
  },
  "parameters" : [
    {
      "id" : "input_obj_ref",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "text",
      "text_options" : {
        "valid_ws_types" : ["KBaseMatrices"]
      }
    },
    {
      "id" : "n_components",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "2" ],
      "field_type" : "text",
      "text_options" : {
        "validate_as": "int"
      }
    },
    {
      "id" : "dimension",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : ["row"],
      "field_type" : "dropdown",
      "dropdown_options": 
      {
        "options": 
        [
          {
              "display": "row",
              "value": "row"
          },
          {
              "display": "column",
              "value": "col"
          }
        ]
      }
    },
    {
      "id" : "attribute_mapping_obj_ref",
      "optional" : true,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "text",
      "text_options" : {
        "valid_ws_types" : ["KBaseExperiments.AttributeMapping"]
      }
    },
    {
      "id" : "attribute_color",
      "optional" : false,
      "advanced" : true,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "textsubdata",
      "textsubdata_options" : {
        "subdata_selection": {
          "parameter_id" : "attribute_mapping_obj_ref",
          "subdata_included" : ["attributes/[*]/attribute"],
          "path_to_subdata": ["attributes"],
          "selection_id": "attribute"
          },
        "multiselection":false,
        "show_src_obj":false,
        "allow_custom":false
      }
    },
    {
      "id" : "attribute_size",
      "optional" : false,
      "advanced" : true,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "textsubdata",
      "textsubdata_options" : {
        "subdata_selection": {
          "parameter_id" : "attribute_mapping_obj_ref",
          "subdata_included" : ["attributes/[*]/attribute"],
          "path_to_subdata": ["attributes"],
          "selection_id": "attribute"
          },
        "multiselection":false,
        "show_src_obj":false,
        "allow_custom":false
      }
    },
    {
      "id" : "nmds_matrix_name",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "text",
      "text_options" : {
          "valid_ws_types" : ["KBaseExperiments.MDSMatrix"],
          "is_output_name" : true
        }
    }
  ],
  "parameter-groups": [
    {
      "id": "color_marker_by",
      "parameters": [
        "attribute_color"
      ],
      "optional": true,
      "allow_multiple": false,
      "with_border": false,
      "advanced" : false
    },
    {
      "id": "scale_size_by",
      "parameters": [
        "attribute_size"
      ],
      "optional": true,
      "allow_multiple": false,
      "with_border": false,
      "advanced" : false
    }
  ],
  "behavior" : {
    "service-mapping" : {
      "url" : "",
      "name" : "GenericsAPI",
      "method" : "run_mds",
      "input_mapping" : [
        {
          "narrative_system_variable": "workspace",
          "target_property": "workspace_name"
        },
        {
          "input_parameter" : "input_obj_ref",
          "target_property" : "input_obj_ref",
          "target_type_transform": "resolved-ref"
        },
        {
          "input_parameter" : "attribute_mapping_obj_ref",
          "target_property" : "attribute_mapping_obj_ref",
          "target_type_transform": "resolved-ref"
        },
        {
            "input_parameter" : "n_components",
            "target_property" : "n_components"
        },
        {
          "input_parameter" : "dimension",
          "target_property" : "dimension"
        },
        {
          "input_parameter" : "scale_size_by",
          "target_property" : "scale_size_by"
        },
        {
          "input_parameter" : "color_marker_by",
          "target_property" : "color_marker_by"
        },
        {
          "input_parameter" : "mds_matrix_name",
          "target_property" : "mds_matrix_name"
        }
      ],
      "output_mapping" : [
        {
          "narrative_system_variable": "workspace",
          "target_property": "wsName"
        },
        {
            "service_method_output_path": [0, "mds_ref"],
            "target_property": "mds_ref"
        },
        {
            "service_method_output_path": [0, "report_name"],
            "target_property": "report_name"
        },
        {
            "service_method_output_path": [0, "report_ref"],
            "target_property": "report_ref"
        },
        {
            "constant_value": "16",
            "target_property": "report_window_line_height"
        }
      ]
    }
  },
  "job_id_output_field": "docker"
}
